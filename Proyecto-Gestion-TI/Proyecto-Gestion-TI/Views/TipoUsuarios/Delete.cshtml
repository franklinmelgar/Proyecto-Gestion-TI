@model Proyecto_Gestion_TI.Models.TipoUsuario

@{
    ViewBag.Titulo = "Eliminar Tipo de Usuario";
}

<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.NombreTipoUsurio)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NombreTipoUsurio)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="CodigoTipoUsuario" />
        <input type="submit" value="Delete" class="btn btn-danger" />
        <a class="btn btn-info btn-small" asp-area="" asp-controller="TipoUsuarios" asp-action="Index">
                <span>Regresar</span>
        </a>
    </form>
</div>
